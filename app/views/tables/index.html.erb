<p id="notice"><%= notice %></p>

<h1>Listado de Tablas</h1>

<table>
  <thead>
    <tr>
      <th>Tab nombre</th>
      <th>Tab nemonico</th>
      <th>Tab descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tables.each do |table| %>
      <tr>
        <td><%= table.tab_nombre %></td>
        <td><%= table.tab_nemonico %></td>
        <td><%= table.tab_descripcion %></td>
        <td><%= link_to 'Show', table %></td>
        <td><%= link_to 'Edit', edit_table_path(table) %></td>
        <td><%= link_to 'Destroy', table, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nueva Tabla implicita', new_table_path %>
